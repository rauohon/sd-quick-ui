{
  "name": "sd-vue-ui",
  "version": "1.0.0",
  "description": "A modern, fast Vue 3 frontend for Stable Diffusion WebUI with Prompt Slots System",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mock": "vite --mode mock",
    "build": "vite build",
    "preview": "vite preview",
    "i18n:verify": "node scripts/verify-translations.js",
    "i18n:validate-usage": "node scripts/validate-translation-usage.js",
    "i18n:validate-params": "node scripts/validate-translation-params.js",
    "i18n:validate": "npm run i18n:verify && npm run i18n:validate-usage && npm run i18n:validate-params",
    "test:i18n": "npm run i18n:validate"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/sd-vue-ui.git"
  },
  "keywords": [
    "stable-diffusion",
    "vue3",
    "webui",
    "ai",
    "image-generation",
    "txt2img",
    "adetailer",
    "prompt-management"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "vue": "^3.5.24",
    "vue-i18n": "^9.14.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "vite": "^7.2.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
